{
  "name": "clean-biolerplate",
  "version": "1.0.0",
  "description": "aplikasi express lebih clean microservices biolderplate",
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest --collectCoverage",
    "prebuild": "rimraf dist/",
    "build": "npx tsc",
    "start": "npm serve",
    "serve": "node dist/index.js",
    "dev": "nodemon",
    "watch-ts": "npx tsc -w",
    "migration:create": "typeorm migration:create",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource src/configs/typeorm.config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource src/configs/typeorm.config.ts"
  },
  "author": "nyonz-hamzah",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/mailgun-js": "^0.22.18",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-joi-validator": "^2.0.1",
    "express-jwt": "^8.5.1",
    "express-winston": "^4.2.0",
    "fs": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^3.0.1",
    "mysql": "^2.18.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3",
    "uuid": "^11.0.4",
    "winston": "^3.17.0"
  }
}
